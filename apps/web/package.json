{
  "name": "property-management-system",
  "version": "1.0.0",
  "description": "Modern property management system with tenant management, meter readings, and billing",
  "private": true,
  "author": "Nuomoria Team",
  "license": "MIT",
  "homepage": "https://nuomoria.lt",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/property-management-system.git"
  },
  "keywords": [
    "property-management",
    "tenant-management",
    "meter-readings",
    "billing",
    "react",
    "typescript",
    "supabase"
  ],
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.53.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^16.18.126",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "chart.js": "^4.5.1",
    "es-toolkit": "^1.41.0",
    "framer-motion": "^12.23.11",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.526.0",
    "nanoid": "^5.1.5",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-intersection-observer": "^9.16.0",
    "react-leaflet": "^5.0.0",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "^5.1.0-next.26",
    "react-window": "^1.8.11",
    "tailwindcss": "^3.4.0",
    "typescript": "^4.9.5",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4",
    "zod": "^3.22.5",
    "zustand": "^5.0.7"
  },
  "scripts": {
    "preinstall": "npm pkg set dependencies.ajv=8.12.0 dependencies.ajv-keywords=5.1.0 || true",
    "start": "react-scripts start",
    "start:dev": "set \"NODE_ENV=development\" && react-scripts start",
    "start:prod": "set \"NODE_ENV=production\" && react-scripts start",
    "build": "react-scripts build",
    "build:dev": "set \"NODE_ENV=development\" && react-scripts build",
    "build:prod": "set \"NODE_ENV=production\" && react-scripts build",
    "test": "react-scripts test",
    "test:dev": "set \"NODE_ENV=development\" && react-scripts test",
    "test:prod": "set \"NODE_ENV=production\" && react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "if exist build rmdir /s /q build && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache",
    "dev": "npm run clean && npm run start:dev",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js' --html build/bundle-report.html",
    "analyze:quick": "npx source-map-explorer 'build/static/js/*.js'",
    "perf": "npm run build && npx lighthouse http://localhost:3000 --preset=desktop --output=json --output-path=./lighthouse.json",
    "perf:mobile": "npm run build && npx lighthouse http://localhost:3000 --preset=mobile --output=html --output-path=./lighthouse-mobile.html",
    "perf:ci": "npx lighthouse http://localhost:3000 --preset=desktop --output=json --output-path=./lh.json --chrome-flags='--headless'",
    "size": "npm run build && npx size-limit",
    "validate": "npm run type-check && npm run lint && npm test -- --watchAll=false",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn",
      "no-console": "warn",
      "@typescript-eslint/no-explicit-any": "warn",
      "no-duplicate-imports": "warn",
      "import/first": "warn",
      "arrow-body-style": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "caniuse-lite": "^1.0.30001731",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.2.4",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "size-limit": [
    {
      "name": "Initial JS Bundle",
      "path": "build/static/js/*.js",
      "limit": "180 KB",
      "gzip": true
    },
    {
      "name": "Initial CSS",
      "path": "build/static/css/*.css",
      "limit": "40 KB",
      "gzip": true
    }
  ],
  "engines": {
    "node": "22.x",
    "npm": ">=8.0.0"
  },
  "resolutions": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}