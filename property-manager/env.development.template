# Development Environment Variables Template
# Copy this to .env.development and fill in your values

# Supabase Configuration (Development/Testing)
REACT_APP_SUPABASE_URL=https://your-dev-project.supabase.co
REACT_APP_SUPABASE_ANON_KEY=your_dev_anon_key_here

# Application Configuration
REACT_APP_APP_URL=http://localhost:3000
REACT_APP_APP_NAME="Property Manager (Dev)"
REACT_APP_AUTH_REDIRECT_URL=http://localhost:3000/auth/callback

# Email Configuration
REACT_APP_EMAIL_FROM=dev@auth.nuomoria.lt
REACT_APP_EMAIL_REPLY_TO=dev-support@nuomoria.lt

# Security
GENERATE_SOURCEMAP=true
REACT_APP_ENV=development

# Analytics and Monitoring (Development - usually disabled)
REACT_APP_GOOGLE_ANALYTICS_ID=
REACT_APP_SENTRY_DSN=

# Feature Flags (Development)
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_DEBUG_MODE=true

# New Feature Flags (Development)
REACT_APP_ENABLE_NEW_DASHBOARD=true
REACT_APP_ENABLE_ADVANCED_ANALYTICS=true
REACT_APP_ENABLE_TENANT_CHAT=true
REACT_APP_ENABLE_MAINTENANCE_SCHEDULING=true
REACT_APP_ENABLE_DOCUMENT_UPLOAD=true
REACT_APP_ENABLE_EXPERIMENTAL_FEATURES=true
REACT_APP_ENABLE_NEW_UI=true
REACT_APP_ENABLE_SIMPLIFIED_WORKFLOW=true

# Performance Settings
REACT_APP_ENABLE_SERVICE_WORKER=false
REACT_APP_CACHE_STRATEGY=minimal


