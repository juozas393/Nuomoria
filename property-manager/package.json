{
  "name": "property-management-system",
  "version": "1.0.0",
  "description": "Modern property management system with tenant management, meter readings, and billing",
  "private": true,
  "author": "Nuomoria Team",
  "license": "MIT",
  "homepage": "https://nuomoria.lt",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/property-management-system.git"
  },
  "keywords": [
    "property-management",
    "tenant-management",
    "meter-readings",
    "billing",
    "react",
    "typescript",
    "supabase"
  ],
  "dependencies": {
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.1",
    "@supabase/supabase-js": "^2.53.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "framer-motion": "^12.23.11",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.526.0",
    "nanoid": "^5.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.62.0",
    "react-intersection-observer": "^9.16.0",
    "react-leaflet": "^5.0.0",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.11",
    "tailwindcss": "^3.4.0",
    "typescript": "^4.9.5",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4",
    "zod": "^3.22.4",
    "zustand": "^5.0.7"
  },
  "scripts": {
    "start": "set \"GENERATE_SOURCEMAP=false\" && react-scripts start",
    "build": "set \"GENERATE_SOURCEMAP=false\" && react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:prod": "set \"GENERATE_SOURCEMAP=false\" && npm run build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "performance": "npm run build && npx lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "clean": "if exist build rmdir /s /q build && if exist node_modules\\.cache rmdir /s /q node_modules\\.cache",
    "dev": "npm run clean && npm start",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.21",
    "caniuse-lite": "^1.0.30001731",
    "postcss": "^8.5.6",
    "postcss-preset-env": "^10.2.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "resolutions": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  }
}
